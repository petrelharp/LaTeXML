<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml package="amsmath"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section refnum="1" xml:id="S1">
    <title><tag close=" ">1</tag>Grouping in multi-relations</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" xml:id="S1.Ex1.m1" tex="a=b=c=d=e" text="a = b = c = d = e">
          <XMath>
            <XMApp>
              <XMTok meaning="multirelation"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">d</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" xml:id="S1.Ex2.m1" tex="a=b,c=d" text="formulae@(a = b, c = d)">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="formulae"/>
                <XMRef idref="XM1"/>
                <XMRef idref="XM2"/>
              </XMApp>
              <XMWrap>
                <XMApp xml:id="XM1">
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                </XMApp>
                <XMTok role="PUNCT">,</XMTok>
                <XMApp xml:id="XM2">
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMApp>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex3">
        <Math mode="display" xml:id="S1.Ex3.m1" tex="a=b,c,d,e" text="a = list@(b, c, d, e)">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMDual>
                <XMApp>
                  <XMTok meaning="list"/>
                  <XMRef idref="XM3"/>
                  <XMRef idref="XM4"/>
                  <XMRef idref="XM5"/>
                  <XMRef idref="XM6"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="ID" xml:id="XM3" font="italic">b</XMTok>
                  <XMTok role="PUNCT">,</XMTok>
                  <XMTok role="ID" xml:id="XM4" font="italic">c</XMTok>
                  <XMTok role="PUNCT">,</XMTok>
                  <XMTok role="ID" xml:id="XM5" font="italic">d</XMTok>
                  <XMTok role="PUNCT">,</XMTok>
                  <XMTok role="ID" xml:id="XM6" font="italic">e</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex4">
        <Math mode="display" xml:id="S1.Ex4.m1" tex="a=b,c,d=e" text="formulae@(a = list@(b, c), d = e)">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="formulae"/>
                <XMRef idref="XM9"/>
                <XMRef idref="XM10"/>
              </XMApp>
              <XMWrap>
                <XMApp xml:id="XM9">
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="list"/>
                      <XMRef idref="XM7"/>
                      <XMRef idref="XM8"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="ID" xml:id="XM7" font="italic">b</XMTok>
                      <XMTok role="PUNCT">,</XMTok>
                      <XMTok role="ID" xml:id="XM8" font="italic">c</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMApp>
                <XMTok role="PUNCT">,</XMTok>
                <XMApp xml:id="XM10">
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                  <XMTok role="ID" font="italic">e</XMTok>
                </XMApp>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex5">
        <Math mode="display" xml:id="S1.Ex5.m1" tex="a=b&gt;c\leq d=e" text="a = b &gt; c less= d = e">
          <XMath>
            <XMApp>
              <XMTok meaning="multirelation"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMTok meaning="greater-than" role="RELOP">&gt;</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
              <XMTok role="ID" font="italic">d</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <paragraph xml:id="S1.SS0.SSS0.Px1">
      <title>But period is somewhat different!</title>
      <para xml:id="S1.SS0.SSS0.Px1.p1">
        <equation xml:id="S1.Ex6">
          <Math mode="display" xml:id="S1.Ex6.m1" tex="a=b,c.d=e" text="formulae@(a = list@(b, c), d = e)">
            <XMath>
              <XMDual>
                <XMApp>
                  <XMTok meaning="formulae"/>
                  <XMRef idref="XM13"/>
                  <XMRef idref="XM14"/>
                </XMApp>
                <XMWrap>
                  <XMApp xml:id="XM13">
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMTok role="ID" font="italic">a</XMTok>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="list"/>
                        <XMRef idref="XM11"/>
                        <XMRef idref="XM12"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok role="ID" xml:id="XM11" font="italic">b</XMTok>
                        <XMTok role="PUNCT">,</XMTok>
                        <XMTok role="ID" xml:id="XM12" font="italic">c</XMTok>
                      </XMWrap>
                    </XMDual>
                  </XMApp>
                  <XMTok role="PERIOD">.</XMTok>
                  <XMApp xml:id="XM14">
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMTok role="ID" font="italic">d</XMTok>
                    <XMTok role="ID" font="italic">e</XMTok>
                  </XMApp>
                </XMWrap>
              </XMDual>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex7">
          <Math mode="display" xml:id="S1.Ex7.m1" tex="a=b.c,d=e" text="formulae@(a = b, list@(c, d) = e)">
            <XMath>
              <XMDual>
                <XMApp>
                  <XMTok meaning="formulae"/>
                  <XMRef idref="XM17"/>
                  <XMRef idref="XM18"/>
                </XMApp>
                <XMWrap>
                  <XMApp xml:id="XM17">
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMTok role="ID" font="italic">a</XMTok>
                    <XMTok role="ID" font="italic">b</XMTok>
                  </XMApp>
                  <XMTok role="PERIOD">.</XMTok>
                  <XMApp xml:id="XM18">
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="list"/>
                        <XMRef idref="XM15"/>
                        <XMRef idref="XM16"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok role="ID" xml:id="XM15" font="italic">c</XMTok>
                        <XMTok role="PUNCT">,</XMTok>
                        <XMTok role="ID" xml:id="XM16" font="italic">d</XMTok>
                      </XMWrap>
                    </XMDual>
                    <XMTok role="ID" font="italic">e</XMTok>
                  </XMApp>
                </XMWrap>
              </XMDual>
            </XMath>
          </Math>
        </equation>
      </para>
    </paragraph>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag close=" ">2</tag>Arrows as meta-relation</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex8">
        <Math mode="display" xml:id="S2.Ex8.m1" tex="a=b\rightarrow c\leq d=e" text="a = b rightarrow c less= d = e">
          <XMath>
            <XMApp>
              <XMTok meaning="multirelation"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMTok name="rightarrow" role="ARROW">→</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
              <XMTok role="ID" font="italic">d</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex9">
        <Math mode="display" xml:id="S2.Ex9.m1" tex="a=b,c\rightarrow d=e" text="formulae@(a = b, c rightarrow d = e)">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="formulae"/>
                <XMRef idref="XM19"/>
                <XMRef idref="XM20"/>
              </XMApp>
              <XMWrap>
                <XMApp xml:id="XM19">
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                </XMApp>
                <XMTok role="PUNCT">,</XMTok>
                <XMApp xml:id="XM20">
                  <XMTok meaning="multirelation"/>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok name="rightarrow" role="ARROW">→</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">e</XMTok>
                </XMApp>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex10">
        <Math mode="display" xml:id="S2.Ex10.m1" tex="a=b\leftrightarrow c\leq d=e" text="a = b leftrightarrow c less= d = e">
          <XMath>
            <XMApp>
              <XMTok name="leftrightarrow" role="METARELOP">↔</XMTok>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMApp>
                <XMTok meaning="multirelation"/>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex11">
        <Math mode="display" xml:id="S2.Ex11.m1" tex="a=b,c\leftrightarrow d=e" text="a = list@(b, c) leftrightarrow d = e">
          <XMath>
            <XMApp>
              <XMTok name="leftrightarrow" role="METARELOP">↔</XMTok>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMDual>
                  <XMApp>
                    <XMTok meaning="list"/>
                    <XMRef idref="XM21"/>
                    <XMRef idref="XM22"/>
                  </XMApp>
                  <XMWrap>
                    <XMTok role="ID" xml:id="XM21" font="italic">b</XMTok>
                    <XMTok role="PUNCT">,</XMTok>
                    <XMTok role="ID" xml:id="XM22" font="italic">c</XMTok>
                  </XMWrap>
                </XMDual>
              </XMApp>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="3" xml:id="S3">
    <title><tag close=" ">3</tag>Other Meta relations</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex12">
        <Math mode="display" xml:id="S3.Ex12.m1" tex="a(&lt;e)" text="annotated@(a, absent less e)">
          <XMath>
            <XMApp>
              <XMTok meaning="annotated"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMDual>
                <XMRef idref="XM23"/>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="XM23">
                    <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                    <XMTok meaning="absent"/>
                    <XMTok role="ID" font="italic">e</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex13">
        <Math mode="display" xml:id="S3.Ex13.m1" tex="a;(&lt;e)" text="annotated@(a, absent less e)">
          <XMath>
            <XMApp>
              <XMTok meaning="annotated"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMDual>
                <XMRef idref="XM24"/>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="XM24">
                    <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                    <XMTok meaning="absent"/>
                    <XMTok role="ID" font="italic">e</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex14">
        <Math mode="display" xml:id="S3.Ex14.m1" tex="a=b+c+d(&lt;e)" text="a = annotated@(b + c + d, absent less e)">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok meaning="annotated"/>
                <XMApp>
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMApp>
                <XMDual>
                  <XMRef idref="XM25"/>
                  <XMWrap>
                    <XMTok role="OPEN" stretchy="false">(</XMTok>
                    <XMApp xml:id="XM25">
                      <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                      <XMTok meaning="absent"/>
                      <XMTok role="ID" font="italic">e</XMTok>
                    </XMApp>
                    <XMTok role="CLOSE" stretchy="false">)</XMTok>
                  </XMWrap>
                </XMDual>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex15">
        <Math mode="display" xml:id="S3.Ex15.m1" tex="a=b+c+d\mod e" text="a = modulo@(b + c + d, e)">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok meaning="modulo" role="MODIFIEROP">mod</XMTok>
                <XMApp>
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMApp>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
</document>
