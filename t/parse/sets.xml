<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" xml:id="S0.Ex1.m1" tex="\{a\}" text="set@(a)">
        <XMath>
          <XMDual>
            <XMApp>
              <XMTok meaning="set"/>
              <XMRef idref="XM1"/>
            </XMApp>
            <XMWrap>
              <XMTok role="OPEN" stretchy="false">{</XMTok>
              <XMTok role="ID" xml:id="XM1" font="italic">a</XMTok>
              <XMTok role="CLOSE" stretchy="false">}</XMTok>
            </XMWrap>
          </XMDual>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex2">
      <Math mode="display" xml:id="S0.Ex2.m1" tex="\{a:a\in b\}" text="conditional-set@(a, a element-of b)">
        <XMath>
          <XMDual>
            <XMApp>
              <XMTok meaning="conditional-set"/>
              <XMRef idref="XM2"/>
              <XMRef idref="XM3"/>
            </XMApp>
            <XMWrap>
              <XMTok role="OPEN" stretchy="false">{</XMTok>
              <XMTok role="ID" xml:id="XM2" font="italic">a</XMTok>
              <XMTok name="colon" role="METARELOP">:</XMTok>
              <XMApp xml:id="XM3">
                <XMTok meaning="element-of" name="in" role="RELOP">∈</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMTok role="CLOSE" stretchy="false">}</XMTok>
            </XMWrap>
          </XMDual>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex3">
      <Math mode="display" xml:id="S0.Ex3.m1" tex="\{a|a\in b\}" text="conditional-set@(a, a element-of b)">
        <XMath>
          <XMDual>
            <XMApp>
              <XMTok meaning="conditional-set"/>
              <XMRef idref="XM4"/>
              <XMRef idref="XM5"/>
            </XMApp>
            <XMWrap>
              <XMTok role="OPEN" stretchy="false">{</XMTok>
              <XMTok role="ID" xml:id="XM4" font="italic">a</XMTok>
              <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              <XMApp xml:id="XM5">
                <XMTok meaning="element-of" name="in" role="RELOP">∈</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMTok role="CLOSE" stretchy="false">}</XMTok>
            </XMWrap>
          </XMDual>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p2">
    <equation xml:id="S0.Ex4">
      <Math mode="display" xml:id="S0.Ex4.m1" tex="\{a|a\in b\}_{c=0}" text="(conditional-set@(a, a element-of b)) _ (c = 0)">
        <XMath>
          <XMApp>
            <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
            <XMDual>
              <XMApp>
                <XMTok meaning="conditional-set"/>
                <XMRef idref="XM6"/>
                <XMRef idref="XM7"/>
              </XMApp>
              <XMWrap>
                <XMTok role="OPEN" stretchy="false">{</XMTok>
                <XMTok role="ID" xml:id="XM6" font="italic">a</XMTok>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMApp xml:id="XM7">
                  <XMTok meaning="element-of" name="in" role="RELOP">∈</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                </XMApp>
                <XMTok role="CLOSE" stretchy="false">}</XMTok>
              </XMWrap>
            </XMDual>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="0" role="NUMBER">0</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
</document>
