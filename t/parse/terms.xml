<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section refnum="1" xml:id="S1">
    <title><tag close=" ">1</tag>Basic math</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" xml:id="S1.Ex1.m1" tex="a+b+c-d+e" text="((a + b + c) - d) + e">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMApp>
                <XMTok meaning="minus" role="ADDOP">-</XMTok>
                <XMApp>
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" xml:id="S1.Ex2.m1" tex="a+bc-de" text="(a + b * c) - d * e">
          <XMath>
            <XMApp>
              <XMTok meaning="minus" role="ADDOP">-</XMTok>
              <XMApp>
                <XMTok meaning="plus" role="ADDOP">+</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex3">
        <Math mode="display" xml:id="S1.Ex3.m1" tex="abc/de" text="((a * b * c) / d) * e">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok mathstyle="inline" meaning="divide" role="MULOP">/</XMTok>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex4">
        <Math mode="display" xml:id="S1.Ex4.m1" tex="a\times b\times c/d\times e" text="((a * b * c) / d) * e">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">×</XMTok>
              <XMApp>
                <XMTok mathstyle="inline" meaning="divide" role="MULOP">/</XMTok>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">×</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <paragraph xml:id="S1.SS0.SSS0.Px1">
      <title>N-ary special cases</title>
      <para xml:id="S1.SS0.SSS0.Px1.p1">
        <equation xml:id="S1.Ex5">
          <Math mode="display" xml:id="S1.Ex5.m1" tex="a\times b\times c\times d" text="a * b * c * d">
            <XMath>
              <XMApp>
                <XMTok meaning="times" role="MULOP">×</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex6">
          <Math mode="display" xml:id="S1.Ex6.m1" tex="(a\times b)\times(c\times d)" text="a * b * c * d">
            <XMath>
              <XMApp>
                <XMTok meaning="times" role="MULOP">×</XMTok>
                <XMDual>
                  <XMRef idref="XM1"/>
                  <XMWrap>
                    <XMTok role="OPEN" stretchy="false">(</XMTok>
                    <XMApp xml:id="XM1">
                      <XMTok meaning="times" role="MULOP">×</XMTok>
                      <XMTok role="ID" font="italic">a</XMTok>
                      <XMTok role="ID" font="italic">b</XMTok>
                    </XMApp>
                    <XMTok role="CLOSE" stretchy="false">)</XMTok>
                  </XMWrap>
                </XMDual>
                <XMDual>
                  <XMRef idref="XM2"/>
                  <XMWrap>
                    <XMTok role="OPEN" stretchy="false">(</XMTok>
                    <XMApp xml:id="XM2">
                      <XMTok meaning="times" role="MULOP">×</XMTok>
                      <XMTok role="ID" font="italic">c</XMTok>
                      <XMTok role="ID" font="italic">d</XMTok>
                    </XMApp>
                    <XMTok role="CLOSE" stretchy="false">)</XMTok>
                  </XMWrap>
                </XMDual>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex7">
          <Math mode="display" xml:id="S1.Ex7.m1" tex="((a\times b)\times(c\times d))" text="a * b * c * d">
            <XMath>
              <XMDual>
                <XMRef idref="XM6"/>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="XM6">
                    <XMTok meaning="times" role="MULOP">×</XMTok>
                    <XMDual>
                      <XMRef idref="XM4"/>
                      <XMWrap>
                        <XMTok role="OPEN" stretchy="false">(</XMTok>
                        <XMApp xml:id="XM4">
                          <XMTok meaning="times" role="MULOP">×</XMTok>
                          <XMTok role="ID" font="italic">a</XMTok>
                          <XMTok role="ID" font="italic">b</XMTok>
                        </XMApp>
                        <XMTok role="CLOSE" stretchy="false">)</XMTok>
                      </XMWrap>
                    </XMDual>
                    <XMDual>
                      <XMRef idref="XM5"/>
                      <XMWrap>
                        <XMTok role="OPEN" stretchy="false">(</XMTok>
                        <XMApp xml:id="XM5">
                          <XMTok meaning="times" role="MULOP">×</XMTok>
                          <XMTok role="ID" font="italic">c</XMTok>
                          <XMTok role="ID" font="italic">d</XMTok>
                        </XMApp>
                        <XMTok role="CLOSE" stretchy="false">)</XMTok>
                      </XMWrap>
                    </XMDual>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
            </XMath>
          </Math>
        </equation>
      </para>
      <para xml:id="S1.SS0.SSS0.Px1.p2">
        <equation xml:id="S1.Ex8">
          <Math mode="display" xml:id="S1.Ex8.m1" tex="a/b/c" text="a / b / c">
            <XMath>
              <XMApp>
                <XMTok mathstyle="inline" meaning="divide" role="MULOP">/</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex9">
          <Math mode="display" xml:id="S1.Ex9.m1" tex="\frac{a}{b}/c" text="a / b / c">
            <XMath>
              <XMApp>
                <XMTok mathstyle="inline" meaning="divide" role="MULOP">/</XMTok>
                <XMApp>
                  <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                </XMApp>
                <XMTok role="ID" font="italic">c</XMTok>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex10">
          <Math mode="display" xml:id="S1.Ex10.m1" tex="a/\frac{b}{c}" text="a / b / c">
            <XMath>
              <XMApp>
                <XMTok mathstyle="inline" meaning="divide" role="MULOP">/</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMApp>
                  <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
      </para>
      <para xml:id="S1.SS0.SSS0.Px1.p3">
        <equation xml:id="S1.Ex11">
          <Math mode="display" xml:id="S1.Ex11.m1" tex="a\times\framebox{$b\times c$}" text="a * b * c">
            <XMath>
              <XMApp>
                <XMTok meaning="times" role="MULOP">×</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMApp enclose="box">
                  <XMTok meaning="times" role="MULOP">×</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
      </para>
    </paragraph>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag close=" ">2</tag>Evaluate At</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex12">
        <Math mode="display" xml:id="S2.Ex12.m1" tex="a|_{\infty}" text="evaluated-at@(a, infinity)">
          <XMath>
            <XMApp>
              <XMTok meaning="evaluated-at"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex13">
        <Math mode="display" xml:id="S2.Ex13.m1" tex="ab|_{a=0}cd|_{c=0}e" text="evaluated-at@(evaluated-at@(a * b, a = 0) * c * d, c = 0) * e">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok meaning="evaluated-at"/>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMApp>
                    <XMTok meaning="evaluated-at"/>
                    <XMApp>
                      <XMTok meaning="times" role="MULOP">⁢</XMTok>
                      <XMTok role="ID" font="italic">a</XMTok>
                      <XMTok role="ID" font="italic">b</XMTok>
                    </XMApp>
                    <XMApp>
                      <XMTok meaning="equals" role="RELOP">=</XMTok>
                      <XMTok role="ID" font="italic">a</XMTok>
                      <XMTok meaning="0" role="NUMBER">0</XMTok>
                    </XMApp>
                  </XMApp>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok meaning="0" role="NUMBER">0</XMTok>
                </XMApp>
              </XMApp>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="3" xml:id="S3">
    <title><tag close=" ">3</tag>Absolute values</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex14">
        <Math mode="display" xml:id="S3.Ex14.m1" tex="|a|" text="absolute-value@(a)">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="absolute-value"/>
                <XMRef idref="XM7"/>
              </XMApp>
              <XMWrap>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMTok role="ID" xml:id="XM7" font="italic">a</XMTok>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex15">
        <Math mode="display" xml:id="S3.Ex15.m1" tex="||a||" text="norm@(a)">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="norm"/>
                <XMRef idref="XM8"/>
              </XMApp>
              <XMWrap>
                <XMTok role="OPEN">||</XMTok>
                <XMTok role="ID" xml:id="XM8" font="italic">a</XMTok>
                <XMTok role="CLOSE">||</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex16">
        <Math mode="display" xml:id="S3.Ex16.m1" tex="|a|_{\infty}" text="(absolute-value@(a)) _ infinity">
          <XMath>
            <XMApp>
              <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
              <XMDual>
                <XMApp>
                  <XMTok meaning="absolute-value"/>
                  <XMRef idref="XM9"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                  <XMTok role="ID" xml:id="XM9" font="italic">a</XMTok>
                  <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                </XMWrap>
              </XMDual>
              <XMTok meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex17">
        <Math mode="display" xml:id="S3.Ex17.m1" tex="|a+b|" text="absolute-value@(a + b)">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="absolute-value"/>
                <XMRef idref="XM10"/>
              </XMApp>
              <XMWrap>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMApp xml:id="XM10">
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                </XMApp>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex18">
        <Math mode="display" xml:id="S3.Ex18.m1" tex="|a-|b||" text="absolute-value@(a - absolute-value@(b))">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="absolute-value"/>
                <XMRef idref="XM13"/>
              </XMApp>
              <XMWrap>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMApp xml:id="XM13">
                  <XMTok meaning="minus" role="ADDOP">-</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="absolute-value"/>
                      <XMRef idref="XM12"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                      <XMTok role="ID" xml:id="XM12" font="italic">b</XMTok>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMApp>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="4" xml:id="S4">
    <title><tag close=" ">4</tag>Failures w/o Backtracking</title>
    <para xml:id="S4.p1">
      <equation xml:id="S4.Ex19">
        <Math mode="display" xml:id="S4.Ex19.m1" tex="||a|-|b||" text="absolute-value@(absolute-value@(a) - absolute-value@(b))">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="absolute-value"/>
                <XMRef idref="XM16"/>
              </XMApp>
              <XMWrap>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMApp xml:id="XM16">
                  <XMTok meaning="minus" role="ADDOP">-</XMTok>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="absolute-value"/>
                      <XMRef idref="XM14"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                      <XMTok role="ID" xml:id="XM14" font="italic">a</XMTok>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                    </XMWrap>
                  </XMDual>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="absolute-value"/>
                      <XMRef idref="XM15"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                      <XMTok role="ID" xml:id="XM15" font="italic">b</XMTok>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMApp>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S4.Ex20">
        <Math mode="display" xml:id="S4.Ex20.m1" tex="||a|-b|" text="absolute-value@(absolute-value@(a) - b)">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="absolute-value"/>
                <XMRef idref="XM19"/>
              </XMApp>
              <XMWrap>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMApp xml:id="XM19">
                  <XMTok meaning="minus" role="ADDOP">-</XMTok>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="absolute-value"/>
                      <XMRef idref="XM18"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                      <XMTok role="ID" xml:id="XM18" font="italic">a</XMTok>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                    </XMWrap>
                  </XMDual>
                  <XMTok role="ID" font="italic">b</XMTok>
                </XMApp>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S4.Ex21">
        <Math mode="display" xml:id="S4.Ex21.m1" tex="|a|b|_{\infty}" text="evaluated-at@(absolute-value@(a) * b, infinity)">
          <XMath>
            <XMApp>
              <XMTok meaning="evaluated-at"/>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMDual>
                  <XMApp>
                    <XMTok meaning="absolute-value"/>
                    <XMRef idref="XM20"/>
                  </XMApp>
                  <XMWrap>
                    <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                    <XMTok role="ID" xml:id="XM20" font="italic">a</XMTok>
                    <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                  </XMWrap>
                </XMDual>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMTok meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
</document>
