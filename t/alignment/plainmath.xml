  <para>
    <equation>
      <Math mode="display" tex="\matrix{a&amp;b\cr c&amp;d}" text="matrix[[a, b], [c, d]]">
        <XMath>
          <XMArray meaning="matrix">
            <XMRow>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">a</XMTok>
              </XMCell>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">b</XMTok>
              </XMCell>
            </XMRow>
            <XMRow>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">c</XMTok>
              </XMCell>
              <XMCell align="center">
                <XMTok role="UNKNOWN" font="italic">d</XMTok>
              </XMCell>
            </XMRow>
          </XMArray>
        </XMath>
      </Math>
    </equation>
