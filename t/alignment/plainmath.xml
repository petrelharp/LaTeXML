  <para>
    <equation>
      <Math mode="display" tex="s(x)=\cases{+1&amp;whatever\cr-1&amp;$x&lt;0$\cr 0}" text="s * x = cases@(+ 1, [whatever], - 1, x less 0, 0, [otherwise])">
        <XMath>
          <XMApp>
            <XMTok meaning="equals" role="RELOP">=</XMTok>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok possibleFunction="yes" role="UNKNOWN" font="italic">s</XMTok>
              <XMDual>
                <XMRef idref="XM7"/>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMTok role="UNKNOWN" xml:id="XM7" font="italic">x</XMTok>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
            <XMDual>
              <XMApp>
                <XMTok meaning="cases"/>
                <XMRef idref="XM2"/>
                <XMRef idref="XM3"/>
                <XMRef idref="XM4"/>
                <XMRef idref="XM5"/>
                <XMRef idref="XM6"/>
                <XMText>
                  <text font="italic">otherwise</text>
                </XMText>
              </XMApp>
              <XMWrap>
                <XMTok role="OPEN">{</XMTok>
                <XMArray>
                  <XMRow>
                    <XMCell align="left">
                      <XMApp xml:id="XM2">
                        <XMTok meaning="plus" role="ADDOP">+</XMTok>
                        <XMTok meaning="1" role="NUMBER">1</XMTok>
                      </XMApp>
                    </XMCell>
                    <XMCell align="left">
                      <XMText xml:id="XM3">whatever</XMText>
                    </XMCell>
                  </XMRow>
                  <XMRow>
                    <XMCell align="left">
                      <XMApp xml:id="XM4">
                        <XMTok meaning="minus" role="ADDOP">-</XMTok>
                        <XMTok meaning="1" role="NUMBER">1</XMTok>
                      </XMApp>
                    </XMCell>
                    <XMCell align="left">
                      <XMApp xml:id="XM5">
                        <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                        <XMTok role="UNKNOWN" font="italic">x</XMTok>
                        <XMTok meaning="0" role="NUMBER">0</XMTok>
                      </XMApp>
                    </XMCell>
                  </XMRow>
                  <XMRow>
                    <XMCell align="left">
                      <XMTok meaning="0" role="NUMBER" xml:id="XM6">0</XMTok>
                    </XMCell>
                    <XMCell/>
                  </XMRow>
                </XMArray>
              </XMWrap>
            </XMDual>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para>
    <equation>
      <Math mode="display" tex="\eqalign{x&amp;\ll y+i+\cdots+y_{n}\cr&amp;\leq z}" text="Array[[x, absent lessless y + i + cdots + y _ n], [, absent less= z]]">
        <XMath>
          <XMArray vattach="baseline">
            <XMRow>
              <XMCell align="right">
                <XMTok role="UNKNOWN" font="italic">x</XMTok>
              </XMCell>
              <XMCell align="left">
                <XMApp>
                  <XMTok meaning="much-less-than" name="ll" role="RELOP">≪</XMTok>
                  <XMTok meaning="absent"/>
                  <XMApp>
                    <XMTok meaning="plus" role="ADDOP">+</XMTok>
                    <XMTok role="UNKNOWN" font="italic">y</XMTok>
                    <XMTok role="UNKNOWN" font="italic">i</XMTok>
                    <XMTok name="cdots" role="ID">⋯</XMTok>
                    <XMApp>
                      <XMTok role="SUBSCRIPTOP" scriptpos="post6"/>
                      <XMTok role="UNKNOWN" font="italic">y</XMTok>
                      <XMTok role="UNKNOWN" font="italic">n</XMTok>
                    </XMApp>
                  </XMApp>
                </XMApp>
              </XMCell>
            </XMRow>
            <XMRow>
              <XMCell/>
              <XMCell align="left">
                <XMApp>
                  <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
                  <XMTok meaning="absent"/>
                  <XMTok role="UNKNOWN" font="italic">z</XMTok>
                </XMApp>
              </XMCell>
            </XMRow>
          </XMArray>
        </XMath>
      </Math>
    </equation>
